{
	"name": "pilotjs-project",
	"version": "0.0.0",
	"private": true,
	"workspaces": {
		"nohoist": ["**"],
		"packages": ["packages/*"]
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"scripts": {
		"publish-canary": "turbo run build && changeset publish --tag canary",
		"publish-next": "turbo run build && changeset publish --tag next",
		"publish-release": "turbo run build && changeset publish"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Wave-Play/pilot.git"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"license": "MIT",
	"author": "WavePlay <dev@waveplay.com> (waveplay.com)",
	"contributors": [
		"Pkmmte Xeleon <pk@waveplay.com>"
	],
	"devDependencies": {
		"turbo": "1.6.3"
	},
	"dependencies": {
		"@changesets/cli": "2.25.2"
	}
}
