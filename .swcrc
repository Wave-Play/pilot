{
	"jsc": {
		"baseUrl": ".",
		"parser": {
			"syntax": "typescript",
			"decorators": true,
			"dynamicImport": true,
			"jsx": true,
			"tsx": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"target": "es2018",
		"keepClassNames": true,
		"loose": true
	},
  "minify": false,
	"module": {
		"type": "commonjs",
		"strict": true,
		"strictMode": true,
		"lazy": false,
		"noInterop": true
	},
	"sourceMaps": true
}
